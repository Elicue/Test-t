{% extends 'base.html.twig' %}

{% block body %}
<div class='error'>
  <div class='img'>
    <img src='{{ asset('img/404.png') }}' alt='error'>
  </div>
  <div class='container'>
    <div class='title'>
      <h1>Oops ! 404</h1>
      <h2>We can't find the page you're looking for.</h2>
    </div>
    <p>Sorry, the page you are looking for does not exist.</p>
    <a class='btn' href='{{ path('account') }}'>Back to home</a>
  </div>
</div>
{% endblock %}
